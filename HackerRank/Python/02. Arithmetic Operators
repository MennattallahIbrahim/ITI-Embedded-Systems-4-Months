a = int(input())
b =  int(input())
addition = a+b 
difference = a-b
multiplication = a*b 
print(addition)
print(difference)
print(multiplication)

''' user friendly
a = int(input("Enter first integer:"))
b =  int(input("Enter sescond integer:"))
sum = a+b 
difference = a-b
multiply = a*b 
print(a ,"+", b  ,"=", sum)
print(a ,"-", b ,"=", difference)
print(a ,"*", b ,"=", multiply)
'''
